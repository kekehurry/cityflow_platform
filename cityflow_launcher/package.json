{
  "name": "cityflow_launcher",
  "version": "0.25.02.23",
  "description": "",
  "author": {
    "name": "Kai Hu",
    "email": "arhukai1111@gmail.com"
  },
  "main": "src/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build-icons": "electron-icon-builder --input=./public/icon.png --output=./assets --flatten",
    "electron": "npm run build && electron .",
    "start": "react-scripts start",
    "dist": "npm run build && electron-builder",
    "dist-win": "npm run build && electron-builder --win --x64"
  },
  "build": {
    "appId": "com.example.cityflow",
    "productName": "CityFlow",
    "copyright": "Copyright Â© 2025 CityFlow",
    "buildVersion": "0.25.02.23",
    "files": [
      "dist/**/*",
      "src/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "/usr/local/bin/docker",
        "to": "docker/darwin/docker"
      },
      {
        "from": "./docker/docker",
        "to": "docker/linux/docker"
      },
      {
        "from": "./docker/docker.exe",
        "to": "docker/win32/docker.exe"
      }
    ],
    "extraMetadata": {
      "main": "src/main.js"
    },
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "style-loader": "^4.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "electron-icon-builder": "^2.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1",
    "three": "^0.173.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
